package tp5ytp6;

import java.util.Scanner;

public class ejercicio4 {

	public static void main(String[] args) {
		Scanner tecla= new Scanner(System.in);
		int num;
		int a[]=new int[20];
		Random random = new Random();

		for(int i =0;i<20;i++) {
//			Math.floor(Math.random() * (MAX - MIN + 1)) + MIN;     GENERADOR DE NUMEROS  ALEATORIOS DENTRO DE UN RANGO
			num=random.nextInt(100 - 1) + 1;
				num=tecla.nextInt();	
				if(num>=1 && num<=100) {
					a[i]=num;
				}
		}
		
	}

}
